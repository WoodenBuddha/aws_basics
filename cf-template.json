{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Resources": {
        "VPC": {
            "Type": "AWS::EC2::VPC",
            "Properties": {
                "CidrBlock": "11.0.0.0/16",
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": "cf-vpc"
                    }
                ]
            }
        },
        "IGW": {
            "Type": "AWS::EC2::InternetGateway",
            "Properties": {
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": "cf-igw"
                    }
                ]
            }
        },
        "VPCIGWATTACHMENT": {
            "Type": "AWS::EC2::VPCGatewayAttachment",
            "Properties": {
                "InternetGatewayId": {
                    "Ref": "IGW"
                },
                "VpcId": {
                    "Ref": "VPC"
                }
            }
        },
        "SUBNETPUBLICA": {
            "Type": "AWS::EC2::Subnet",
            "Properties": {
                "VpcId": {
                    "Ref": "VPC"
                },
                "AvailabilityZone": "eu-west-1a",
                "CidrBlock": "11.0.11.0/24",
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": "cf-subnet-public-a"
                    }
                ]
            }
        },
        "SUBNETPUBLICB": {
            "Type": "AWS::EC2::Subnet",
            "Properties": {
                "VpcId": {
                    "Ref": "VPC"
                },
                "AvailabilityZone": "eu-west-1b",
                "CidrBlock": "11.0.21.0/24",
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": "cf-subnet-public-b"
                    }
                ]
            }
        },
        "SUBNETPRIVATEA": {
            "Type": "AWS::EC2::Subnet",
            "Properties": {
                "VpcId": {
                    "Ref": "VPC"
                },
                "AvailabilityZone": "eu-west-1a",
                "CidrBlock": "11.0.12.0/24",
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": "cf-subnet-private-a"
                    }
                ]
            }
        },
        "SUBNETPRIVATEB": {
            "Type": "AWS::EC2::Subnet",
            "Properties": {
                "VpcId": {
                    "Ref": "VPC"
                },
                "AvailabilityZone": "eu-west-1b",
                "CidrBlock": "11.0.2.0/24",
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": "cf-subnet-private-b"
                    }
                ]
            }
        },
        "RTPUBLIC": {
            "Type": "AWS::EC2::RouteTable",
            "Properties": {
                "VpcId": {
                    "Ref": "VPC"
                },
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": "cf-rt-public"
                    }
                ]
            }
        },
        "IGWROUTE": {
            "Type": "AWS::EC2::Route",
            "Properties": {
                "RouteTableId": {
                    "Ref": "RTPUBLIC"
                },
                "DestinationCidrBlock": "0.0.0.0/0",
                "GatewayId": {
                    "Ref": "IGW"
                }
            }
        },
        "PRIVATERTA": {
            "Type": "AWS::EC2::RouteTable",
            "Properties": {
                "VpcId": {
                    "Ref": "VPC"
                },
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": "cf-rt-private-a"
                    }
                ]
            }
        },
        "NATA": {
            "Type": "AWS::EC2::NatGateway",
            "Properties": {
                "AllocationId": {
                    "Fn::GetAtt": [
                        "EIP1",
                        "AllocationId"
                    ]
                },
                "SubnetId": {
                    "Ref": "SUBNETPUBLICA"
                }
            }
        },
        "EIP1": {
            "Type": "AWS::EC2::EIP",
            "Properties": {
                "Domain": "vpc"
            }
        },
        "NATAROUTE": {
            "Type": "AWS::EC2::Route",
            "Properties": {
                "RouteTableId": {
                    "Ref": "PRIVATERTA"
                },
                "DestinationCidrBlock": "0.0.0.0/0",
                "NatGatewayId": {
                    "Ref": "NATA"
                }
            }
        },
        "PRIVATERTB": {
            "Type": "AWS::EC2::RouteTable",
            "Properties": {
                "VpcId": {
                    "Ref": "VPC"
                },
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": "cf-rt-private-b"
                    }
                ]
            }
        },
        "NATB": {
            "Type": "AWS::EC2::NatGateway",
            "Properties": {
                "AllocationId": {
                    "Fn::GetAtt": [
                        "EIP2",
                        "AllocationId"
                    ]
                },
                "SubnetId": {
                    "Ref": "SUBNETPUBLICB"
                }
            }
        },
        "EIP2": {
            "Type": "AWS::EC2::EIP",
            "Properties": {
                "Domain": "vpc"
            }
        },
        "NATBROUTE": {
            "Type": "AWS::EC2::Route",
            "Properties": {
                "RouteTableId": {
                    "Ref": "PRIVATERTB"
                },
                "DestinationCidrBlock": "0.0.0.0/0",
                "NatGatewayId": {
                    "Ref": "NATB"
                }
            }
        },
        "EC2INSTANCE1A": {
            "Type": "AWS::EC2::Instance",
            "Properties": {
                "KeyName": "ec2-linux",
                "ImageId": "ami-0ac3f325873889036",
                "InstanceType": "t2.micro",
                "SubnetId": {
                    "Ref": "SUBNETPRIVATEA"
                }
        }
    }
}