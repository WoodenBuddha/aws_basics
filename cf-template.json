{
    "AWSTemplateFormatVersion": "2010-09-09",
	"Parameters" : {
	  "DefaultVpcCidrBlock" : {
		"Type" : "String",
		"Default" : "11.0.0.0/16",
		"Description" : "Default is 11.0.0.0/16."
	  },
	  "DefaultAZ" : {
		"Type" : "String",
		"Default" : "eu-west-1a"
	  },
	 
    "VPCSecurityGroups": {
      "Type": "List<AWS::EC2::SecurityGroup::Id>",
      "Description": "The list of security groups to associate with this instance"
    }
	},
    "Resources": {
        "VPC": {
            "Type": "AWS::EC2::VPC",
            "Properties": {
                "CidrBlock": { "Ref" : "DefaultVpcCidrBlock" }, 
                "Tags": [
                    {
                        "Key": "Name",
                        "Value":{ "Fn::Join" : [ "", [ {"Ref" : "AWS::StackName"}, "-vpc" ] ] }
                    }
                ]
            }
        },
        "IGW": {
            "Type": "AWS::EC2::InternetGateway",
            "Properties": {
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": "cf-igw"
                    }
                ]
            }
        },
        "VPCIGWATTACHMENT": {
            "Type": "AWS::EC2::VPCGatewayAttachment",
            "Properties": {
                "InternetGatewayId": {
                    "Ref": "IGW"
                },
                "VpcId": {
                    "Ref": "VPC"
                }
            }
        },
        "SUBNETPUBLICA": {
            "Type": "AWS::EC2::Subnet",
            "Properties": {
                "VpcId": {
                    "Ref": "VPC"
                },
                "CidrBlock": "11.0.11.0/24",
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": { "Fn::Join" : [ "", [ {"Ref" : "AWS::StackName"}, "-subnet-public-a" ] ] }
                    }
                ]
            }
        },
        "SUBNETPUBLICB": {
            "Type": "AWS::EC2::Subnet",
            "Properties": {
                "VpcId": {
                    "Ref": "VPC"
                },
				"AvailabilityZone" : {"Ref":"DefaultAZ"},
                "CidrBlock": "11.0.21.0/24",
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": { "Fn::Join" : [ "", [ {"Ref" : "AWS::StackName"}, "-subnet-public-b" ] ] }
                    }
                ]
            }
        },
        "SUBNETPRIVATEA": {
            "Type": "AWS::EC2::Subnet",
            "Properties": {
                "VpcId": {
                    "Ref": "VPC"
                },
                "AvailabilityZone" : {"Ref":"DefaultAZ"},
                "CidrBlock": "11.0.12.0/24",
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": { "Fn::Join" : [ "", [ {"Ref" : "AWS::StackName"}, "-subnet-private-a" ] ] }
                    }
                ]
            }
        },
        "SUBNETPRIVATEB": {
            "Type": "AWS::EC2::Subnet",
            "Properties": {
                "VpcId": {
                    "Ref": "VPC"
                },
				"AvailabilityZone" : "eu-west-1b",
                "CidrBlock": "11.0.2.0/24",
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": { "Fn::Join" : [ "", [ {"Ref" : "AWS::StackName"}, "-subnet-private-b" ] ] }
                    }
                ]
            }
        },
        "RTPUBLIC": {
            "Type": "AWS::EC2::RouteTable",
            "Properties": {
                "VpcId": {
                    "Ref": "VPC"
                },
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": { "Fn::Join" : [ "", [ {"Ref" : "AWS::StackName"}, "-routetable-public" ] ] }
                    }
                ]
            }
        },
		"PRIVATESUBNET1APRIVATERT": {
		  "Type" : "AWS::EC2::SubnetRouteTableAssociation",
		  "Properties" : {
			  "RouteTableId" : { "Ref" : "PRIVATERTA" },
			  "SubnetId" : { "Ref" : "SUBNETPRIVATEA" }
			}
		},
		"PRIVATESUBNET1BPRIVATERT": {
		  "Type" : "AWS::EC2::SubnetRouteTableAssociation",
		  "Properties" : {
			  "RouteTableId" : { "Ref" : "PRIVATERTA" },
			  "SubnetId" : { "Ref" : "SUBNETPRIVATEB" }
			}
		},
		"SUBNETPUBLICB2ARTPUBLIC": {
		  "Type" : "AWS::EC2::SubnetRouteTableAssociation",
		  "Properties" : {
			  "RouteTableId" : { "Ref" : "RTPUBLIC" },
			  "SubnetId" : { "Ref" : "SUBNETPUBLICA" }
			}
		},
		"SUBNETPUBLICB2BRTPUBLIC": {
		  "Type" : "AWS::EC2::SubnetRouteTableAssociation",
		  "Properties" : {
			  "RouteTableId" : { "Ref" : "RTPUBLIC" },
			  "SubnetId" : { "Ref" : "SUBNETPUBLICB" }
			}
		},
        "IGWROUTE": {
            "Type": "AWS::EC2::Route",
            "Properties": {
                "RouteTableId": {
                    "Ref": "RTPUBLIC"
                },
                "DestinationCidrBlock": "0.0.0.0/0",
                "GatewayId": {
                    "Ref": "IGW"
                }
            }
        },
        "PRIVATERTA": {
            "Type": "AWS::EC2::RouteTable",
            "Properties": {
                "VpcId": {
                    "Ref": "VPC"
                },
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": { "Fn::Join" : [ "", [ {"Ref" : "AWS::StackName"}, "-routetable-private-a" ] ] }
                    }
                ]
            }
        },
        "NATA": {
            "Type": "AWS::EC2::NatGateway",
            "Properties": {
                "AllocationId": {
                    "Fn::GetAtt": [
                        "EIP1",
                        "AllocationId"
                    ]
                },
                "SubnetId": {
                    "Ref": "SUBNETPUBLICA"
                }
            }
        },
        "EIP1": {
            "Type": "AWS::EC2::EIP",
            "Properties": {
                "Domain": "vpc"
            }
        },
        "NATAROUTE": {
            "Type": "AWS::EC2::Route",
            "Properties": {
                "RouteTableId": {
                    "Ref": "PRIVATERTA"
                },
                "DestinationCidrBlock": "0.0.0.0/0",
                "NatGatewayId": {
                    "Ref": "NATA"
                }
            }
        },
        "PRIVATERTB": {
            "Type": "AWS::EC2::RouteTable",
            "Properties": {
                "VpcId": {
                    "Ref": "VPC"
                },
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": { "Fn::Join" : [ "", [ {"Ref" : "AWS::StackName"}, "-routetable-private-b" ] ] }
                    }
                ]
            }
        },
        "NATB": {
            "Type": "AWS::EC2::NatGateway",
            "Properties": {
                "AllocationId": {
                    "Fn::GetAtt": [
                        "EIP2",
                        "AllocationId"
                    ]
                },
                "SubnetId": {
                    "Ref": "SUBNETPUBLICB"
                }
            }
        },
        "EIP2": {
            "Type": "AWS::EC2::EIP",
            "Properties": {
                "Domain": "vpc"
            }
        },
        "NATBROUTE": {
            "Type": "AWS::EC2::Route",
            "Properties": {
                "RouteTableId": {
                    "Ref": "PRIVATERTB"
                },
                "DestinationCidrBlock": "0.0.0.0/0",
                "NatGatewayId": {
                    "Ref": "NATB"
                }
            }
        },
		"EIPEC2": {
            "Type": "AWS::EC2::EIP",
            "Properties": {
                "Domain": "vpc",
				"InstanceId": {"Ref":"EC2INSTANCE2A"}
            }
        },
        "EC2INSTANCE1A": {
            "Type": "AWS::EC2::Instance",
            "Properties": {
                "KeyName": "ec2-linux",
                "ImageId": "ami-0ac3f325873889036",
				"IamInstanceProfile" : {
				  "Ref" : "S3BucketsInstanceProfile"
				},
                "InstanceType": "t2.micro",
                "SubnetId": {
                    "Ref": "SUBNETPRIVATEA"
                },
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": { "Fn::Join" : [ "", [ {"Ref" : "AWS::StackName"}, "-ec2-1a" ] ] }
                    }
                ]
			}
        },
		"EC2INSTANCE1B": {
            "Type": "AWS::EC2::Instance",
            "Properties": {
                "KeyName": "ec2-linux",
                "ImageId": "ami-0ac3f325873889036",
				"IamInstanceProfile" : {
				  "Ref" : "S3BucketsInstanceProfile"
				},
                "InstanceType": "t2.micro",
                "SubnetId": {
                    "Ref": "SUBNETPRIVATEB"
                },
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": { "Fn::Join" : [ "", [ {"Ref" : "AWS::StackName"}, "-ec2-1b" ] ] }
                    }
                ]
			}
        },
		"EC2INSTANCE2A": {
            "Type": "AWS::EC2::Instance",
            "Properties": {
				"AvailabilityZone" : {"Ref":"DefaultAZ"},
                "KeyName": "ec2-linux",
                "ImageId": "ami-0ac3f325873889036",
				"IamInstanceProfile" : {
				  "Ref" : "S3BucketsInstanceProfile"
				},
                "InstanceType": "t2.micro",
                "SubnetId": {
                    "Ref": "SUBNETPUBLICA"
                },
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": { "Fn::Join" : [ "", [ {"Ref" : "AWS::StackName"}, "-ec2-2a" ] ] }
                    }
                ]
			}
        },
		"EC2INSTANCE2B": {
            "Type": "AWS::EC2::Instance",
            "Properties": {
                "KeyName": "ec2-linux",
                "ImageId": "ami-0ac3f325873889036",
				"IamInstanceProfile" : {
				  "Ref" : "S3BucketsInstanceProfile"
				},
                "InstanceType": "t2.micro",
                "SubnetId": {
                    "Ref": "SUBNETPUBLICB"
                },
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": { "Fn::Join" : [ "", [ {"Ref" : "AWS::StackName"}, "-ec2-2b" ] ] }
                    }
                ]
			}
        },
		"S3BucketsInstanceProfile" : {
		  "Type" : "AWS::IAM::InstanceProfile",
		  "Properties" : {
			"Path" : "/",
			"Roles" : ["ec2-s3-readwrite-role"]
		  }
		},
		
		"DB" : {
			 "Type" : "AWS::RDS::DBInstance",
			"Properties": 
			{
				"DBInstanceIdentifier": "pgdb",
				"DBInstanceClass": "db.t2.micro",
				"Engine": "postgres",
				"MasterUsername": "postgres",
				"MasterUserPassword" : "p0stgr3s",
				"AllocatedStorage": 20,
				"DBSubnetGroupName" : { "Ref" : "DBSubnetGroup" },
				"DBSecurityGroups": [],
				"AvailabilityZone" : {"Ref":"DefaultAZ"}
			}
		},
		
		
		"DBSubnetGroup": {
		  "Type" : "AWS::RDS::DBSubnetGroup",
		  "Properties" : {
			  "DBSubnetGroupDescription": "description",
			  "SubnetIds" : [
			  {"Ref": "SUBNETPUBLICB"}, {"Ref": "SUBNETPRIVATEB"}
			  ]
			}
		}
		
    
	
    }
}